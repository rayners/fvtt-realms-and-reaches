{
  "name": "realms-and-reaches",
  "version": "0.1.0",
  "description": "A queryable biome and terrain layer for Foundry VTT",
  "type": "module",
  "main": "dist/module.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "link": "./link-module.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js}\" \"*.{js,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run lint && npm run format:check && npm run test:run && npm run build",
    "prepare": "npm run build"
  },
  "author": "David Raynes <rayners@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@ethaks/fvtt-quench": "^0.10.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "fvtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "rollup": "^3.20.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-serve": "^2.0.2",
    "sass": "^1.62.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^3.2.2"
  }
}
